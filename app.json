      "bundleIdentifier": "com.andiamoProjectApp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "AndiamoProject.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}